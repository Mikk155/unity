        "Target"
    ],
    "data": {
        "message": {
            "variable": "string",
            "title": "env_message::message",
            "description": "env_message::message::description"
        },
        "spawnflags": {
            "choices": {
                "1": {
                    "title": "env_message::spawnflags::1",
                    "description": "env_message::spawnflags::1::description"
                },
                "2": {
                    "title": "env_message::spawnflags::2",
                    "description": "env_message::spawnflags::2::description"
